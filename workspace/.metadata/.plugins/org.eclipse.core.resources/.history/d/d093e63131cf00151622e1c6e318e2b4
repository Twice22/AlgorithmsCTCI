package permutation;

import java.util.ArrayList;

public class permutationString {
	
	static String[] permutation(String str) {
		ArrayList<StringBuilder> nStr = new ArrayList<StringBuilder>();
		
		if ( str.length() == 1 ) {
			nStr.add(str);
		} else {
			str = str.substring(1);
			for ( String allStr : nStr ) {
				for (int i = 0; i < allStr.length(); i++) {
					nStr.add()
				}
			}
		}
		
	}
	
	public static void main (String args[]) {
		StringBuilder str = new StringBuilder("apud");
		permutation(str.toString());
	}
}
