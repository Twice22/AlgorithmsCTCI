package permutation;

import java.util.ArrayList;

public class permutationString {
	
	static String[] permutation(String str) {
		ArrayList<String> nStr = new ArrayList<String>();
		
		if ( str.length() == 1 ) {
			nStr.add(str);
		} else {
			str = str.substring(1);
		}
		
	}
	
	public static void main (String args[]) {
		String str = "apud";
		permutation(str);
	}
}
